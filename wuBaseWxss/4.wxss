.banner {
    width: 100%;
    height: 280rpx;
    position: relative;
    background: #595457;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
}

.banner image {
    position: absolute;
    left: 0;
    top: 0;
    opacity: 0.6;
    animation: banner linear 20s forwards;
}

@-webkit-keyframes banner {
    0% {
        -webkit-transform: scale(3);
    }

    100% {
        -webkit-transform: scale(1);
    }
}

@keyframes banner {
    0% {
        transform: scale(3);
    }

    100% {
        transform: scale(1);
    }
}

.banner-font {
    color: #fff;
    font-size: 56rpx;
    font-weight: bold;
    opacity: 0;
    animation: banner-font ease 6s 1s forwards;
    position: relative;
}

@-webkit-keyframes banner-font {
    0% {
        -webkit-transform: scale(9);
        opacity: 0;
    }

    30% {
        -webkit-transform: scale(0.8);
        opacity: 1;
    }

    65% {
        -webkit-transform: scale(1.1);
        opacity: 1;
    }

    100% {
        -webkit-transform: scale(1);
        opacity: 1;
    }
}

@keyframes banner-font {
    0% {
        transform: scale(9);
        opacity: 0;
    }

    30% {
        transform: scale(0.8);
        opacity: 1;
    }

    65% {
        transform: scale(1.1);
        opacity: 1;
    }

    100% {
        transform: scale(1);
        opacity: 1;
    }
}

.banner-font::after,.banner-font::before {
    position: absolute;
    content: "";
    display: block;
    height: 1px;
    background: rgba(255,255,255,0.6);
    width: 0;
    top: 50%;
    animation: font-line 1s ease-in-out 4s forwards;
}

.banner-font::after {
    right: 0;
    transform: translateX(110%);
}

.banner-font::before {
    left: 0;
    transform: translateX(-110%);
}

@-webkit-keyframes font-line {
    0% {
        width: 0;
    }

    100% {
        width: 150rpx;
    }
}

@keyframes font-line {
    0% {
        width: 0;
    }

    100% {
        width: 150rpx;
    }
}

.section-nav {
    white-space: nowrap;
    width: 100%;
    overflow: hidden;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    height: 92rpx;
    line-height: 92rpx;
    background: #f5f5f5;
}

.section-nav .section-nav-li {
    padding: 0px 30rpx;
    display: inline-block;
    position: relative;
    position: relative;
    font-size: 36rpx;
}

.section-nav-li:active {
    background: rgba(0,0,0,0.05);
}

.section-nav-li.active {
    color: #312e43;
    font-weight: bold;
}

.section-nav-li::after {
    position: absolute;
    content: "";
    display: block;
    left: -100rpx;
    right: -100rpx;
    height: 3px;
    bottom: 0;
    background: #312e43;
    transition: all ease 0.3s;
    opacity: 0;
}

.section-nav-li.active::after {
    left: 30rpx;
    right: 30rpx;
    opacity: 1;
}

.section-nav .section-li-box {
    display: block;
}

