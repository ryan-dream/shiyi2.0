@import "../../../resource/wxParse/wxParse.wxss";

.topBg {
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 40rpx;
    background-color: #07bb45;
}

.elliBg {
    position: absolute;
    top: -;
    left: 0;
    width: 100%;
    height: 80rpx;
    border-radius: 50%;
    background-color: #07bb45;
}

.page {
    margin: 20rpx 30rpx;
    width: auto;
    background-color: #fff;
    padding: 0 20rpx;
    border-radius: 12rpx;
}

.title {
    font-size: 32rpx;
    padding: 20rpx 0;
}

.content {
    color: #666;
    text-align: justify;
    padding: 30rpx 0;
    text-indent: 30rpx;
}