<view class="tabbar_top" style="--color:{{color}};">
    <view class="topBox white_bg mb15">
        <view class="totalProfit gridXb flex ">
            <view class="">累计总收益</view>
            <view>
                <text class="proVal subColor">{{datum.money?datum.money:'0.00'}}</text>
                <text> 元</text>
            </view>
        </view>
    </view>
    <view class="topBox white_bg mb15">
        <view class="totalProfit gridXb flex ">
            <view class="">可提现金额</view>
            <view>
                <text class="proVal subColor">{{datum.can_money?datum.can_money:'0.00'}}</text>
                <text> 元</text>
            </view>
        </view>
        <view class="ruleBox f14 grey">
            {{datum.modules.cash_explain}}
        </view>
    </view>
    <view class="txBtnBox mb15">
        <button bindtap="getcash" class="txBtn global">申请提现</button>
    </view>
    <view class="topBox white_bg mb15">
        <view class="totalProfit gridXb flex ">
            <view class="">已提现金额</view>
            <view>
                <text class="proVal subColor">{{datum.use_money?datum.use_money:'0.00'}}</text>
                <text> 元</text>
            </view>
        </view>
        <view class="ruleBox f14 grey">
            请到微信钱包查看具体收益
        </view>
    </view>
</view>
