.playH,.homeH {
    position: fixed;
    bottom: 80px;
    right: 20px;
    z-index: 1000;
    width: 2rem;
    height: 2rem;
    color: #fff;
    background: rgba(8,186,6,1);
    line-height: 1.9rem;
    text-align: center;
    border-radius: 100%;
}

.playH {
    overflow: hidden;
    line-height: 1.5rem;
}

.playH image {
    width: 60%;
    height: 60%;
}

.icon-shouye {
    font-weight: 600;
}

.playH-box {
    position: relative;
    overflow: hidden;
}

.line-cen {
    position: absolute;
    bottom: 24rpx;
    left: 0;
    z-index: 200;
    width: 1.6rem;
    height: 3px;
    background: rgba(8,186,6,1);
    overflow: visible;
}

.playH-box .line {
    display: inline-block;
    width: 3px;
    height: 10px;
    background-color: #fff;
    margin-right: 2px;
    overflow: hidden;
}

@keyframes line {
    from {
        height: 0;
    }

    to {
        height: 16px;
    }
}

@-webkit-keyframes line {
    from {
        height: 2px;
    }

    to {
        height: 16px;
    }
}

.playH-box .line1 {
    animation: line 0.6s infinite ease-in-out alternate;
    overflow: hidden;
}

.playH-box .line2 {
    animation: line 0.6s 0.2s infinite ease-in-out alternate;
    overflow: hidden;
}

.playH-box .line3 {
    animation: line 0.6s 0.4s infinite ease-in-out alternate;
    overflow: hidden;
}

.playH-box .line4 {
    animation: line 0.6s 0.1s infinite ease-in-out alternate;
    overflow: hidden;
}

