.columnHeaer .infoIn .have-haircut-up {
    content: "";
    position: absolute;
    display: block;
    width: 40rpx;
    height: 40rpx;
    border: 3px solid;
    border-color: #999 #999 transparent transparent;
    bottom: -50rpx;
    left: 50%;
    transform: translateY(-0%) scale(0.7) rotate(135deg);
    transition: all 0.3s ease;
    border-radius: 2px;
}

.columnHeaer .infoIn .have-haircut-arrows {
    transform: translateY(-0%) scale(0.7) rotate(-45deg);
}

.columnHeaer .infoIn .have-haircut-down {
    position: absolute;
    display: block;
    width: 40rpx;
    height: 40rpx;
    border: 3px solid;
    border-color: #999 #999 transparent transparent;
    bottom: -16%;
    left: 50%;
    margin-left: -20rpx;
    transform: translateY(-50%) scale(0.7) rotate(-45deg);
    transition: all 0.3s ease;
    border-radius: 2px;
}

.columnHeaer .infoIn .text.active {
    height: auto;
}

.column .infoIn .Arrow {
