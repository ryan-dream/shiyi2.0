}

.playH {
    overflow: hidden;
    line-height: 1.5rem;
}

.playH image {
    width: 45%;
    height: 45%;
}

.playH-box {
    position: relative;
    overflow: visible;
}

.line-cen {
    position: absolute;
    bottom: 24rpx;
    left: 0;
    z-index: 200;
    width: 1.6rem;
    height: 3px;
    background: rgba(8,186,6,1);
