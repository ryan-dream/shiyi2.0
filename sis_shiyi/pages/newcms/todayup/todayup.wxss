.Elastic {
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.8);
    width: 100vw;
    height: 100vh;
    z-index: 999;
    -webkit-animation: popupbg 0.4s ease forwards;
    opacity: 0;
}

.Elastic.active {
    display: block;
}

.ElasticMin {
    width: 100vw;
    height: auto;
    -webkit-animation: popup 0.4s ease forwards;
    -webkit-opacity: 0;
}

.ElasticMin.active {
    padding: 0 15px 15px 15px;
    transform: translateY(0vh);
}

.ElasticMin .tit {
    padding: 15px;
}

.ElasticMin .close {
    width: 0;
    height: 0;
    border: 20px solid;
    position: absolute;
    top: 0;
    right: 0;
    background: none;
    border-radius: inherit;
}

.ElasticMin .close text.iconfont {
    position: absolute;
    top: -16px;
    left: 0;
    color: #fff;
}

.ElasticMin .textarea {
    background: rgba(7,187,69,0.03);
}

.ElasticMin .btn button {
    width: 100%;
    height: 46px;
    line-height: 46px;
    background: #07BB45;
    color: #fff;
    border-radius: 3px;
}

.popup {
    -webkit-animation: popupbg 0.4s ease forwards;
    -webkit-opacity: 0;
}

@-webkit-keyframes popupbg {
    0% {
        -webkit-opacity: 0;
    }

    100% {
        -webkit-opacity: 1;
    }
}

.popup .popup-box {
    -webkit-animation: popup 0.4s ease forwards;
    -webkit-transform: translateY(36vh);
    -webkit-opacity: 0;
}

@-webkit-keyframes popup {
    0% {
        -webkit-transform: translateY(36vh);
        -webkit-opacity: 0;
    }

    100% {
        -webkit-transform: translateY(0);
        -webkit-opacity: 1;
    }
}

