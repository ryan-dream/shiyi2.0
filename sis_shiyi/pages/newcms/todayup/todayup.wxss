    height: 40rpx;
    bottom: 0px;
    right: 2px;
    z-index: 99;
}

.column .infoIn .Arrow::before {
    transform: translateY(-50%) scale(0.8) rotate(135deg);
    right: 0;
}

.column .infoIn .Arrow.active {
